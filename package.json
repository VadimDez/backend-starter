  },
  "homepage": "https://github.com/VadimDez/backend-starter#readme",
  "dependencies": {
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.3",
    "express-openapi-validate": "^0.6.1",
    "helmet": "^5.0.2",
    "http-status": "^1.5.0",
    "ibmcloud-appid": "^6.2.5",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.19",
